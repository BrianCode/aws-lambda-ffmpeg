  "main": "index.js",
  "scripts": {
    "postinstall": "./scripts/postinstall.js",
    "test": "ava"
  },
  "keywords": [
    "aws",
    "lambda",
    "ffmpeg",
    "gcp",
    "gcf",
    "cloud function",
    "azure function"
  ],
  "author": "Barrett Harber",
  "license": "Unlicense",
  "devDependencies": {
    "ava": "^0.17.0",
    "aws-sdk-on-lambda": "*",
    "azure-storage": "^1.4.0",
    "babel-cli": "^6.22.2",
    "babel-preset-env": "^1.1.8",
    "del": "^2.2.0",
    "google-cloud": "^0.45.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-chmod": "^2.0.0",
    "gulp-install": "^0.6.0",
    "gulp-json-editor": "^2.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-shell": "^0.5.2",
    "gulp-util": "^3.0.7",
    "gulp-zip": "^3.0.2",
    "run-sequence": "^1.0.2"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          },
          "exclude": [
            "transform-es2015-arrow-functions",
            "transform-es2015-for-of",
            "check-es2015-constants"
          ]
        }
      ]
    ]
  },
  "ava": {
    "files": "test/unit/*.js",
    "require": [
      "babel-register"
